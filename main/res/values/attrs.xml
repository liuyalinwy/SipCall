<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlashButton">
        <attr name="type" >
            <enum name="Text" value="0">
            </enum>
            <enum name="Image" value="1">
            </enum>
        </attr>
        <attr name="interval" format="integer"/>
        <attr name="textColor1" format="color"/>
        <attr name="textColor2" format="color"/>
        <attr name="backColor1" format="color"/>
        <attr name="backColor2" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="imageId1" format="reference|color"/>
        <attr name="imageId2" format="reference|color"/>
    </declare-styleable>
</resources>